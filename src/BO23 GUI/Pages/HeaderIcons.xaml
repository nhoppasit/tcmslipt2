<UserControl x:Class="BO23_GUI_idea.Pages.HeaderIcons"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:bhm="clr-namespace:BO23_GUI_idea.Pages"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignWidth="{StaticResource Scr_W}" d:DesignHeight="{StaticResource Header_H}" >

    <!--Resource-->
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="BHMDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    
    <Grid Width="{StaticResource Scr_W}" Height="{StaticResource Header_H}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="180" />
            <ColumnDefinition Width="336*" />
            <ColumnDefinition Width="230" />
        </Grid.ColumnDefinitions>
        <bhm:BHM_logo Grid.Column="0" Background="Transparent" Margin="10,0,10,0"/>
        <bhm:NPT_Logo Grid.Column="2" Margin="10,-10,0,0"/>
    </Grid>
    
    
</UserControl>
